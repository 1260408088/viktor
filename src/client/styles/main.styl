@import "variables"
@import "daw/*"

html,
body,
h1, h2, h3, h4, h5, h6
	font-weight: 100

html,
body
	background-color: black

.inline-block
	display: inline-block

.no-padding
	padding: 0

.noselect
	-webkit-touch-callout: none
	-webkit-user-select: none
	-khtml-user-select: none
	-moz-user-select: none
	-ms-user-select: none
	user-select: none